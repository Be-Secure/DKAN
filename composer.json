{
    "name": "getdkan/dkan2",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "description": "DKAN Open Data Catalog",
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.5.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "drupal/jsonb": "1.x-dev",
        "drupal/core": "~8.5.2",
        "drupal/entity": "1.0-beta3",
        "drupal/features": "3.x-dev"
    },
    "require-dev": {
      "burdamagazinorg/thunder-dev-tools": "dev-master",
      "drupal/coder": "8.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "post-install-cmd": ["@composer drupal-scaffold"],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "enable-asset-packagist": "Social\\Composer\\AssetPackagist::execute"
    },
    "autoload": {
        "psr-4": {
            "Social\\": "src"
        }
    },
    "extra": {
        "enable-patching": true,
        "installer-paths": {
          "docroot/core": ["type:drupal-core"],
          "docroot/libraries/{$name}": ["type:drupal-library"],
          "docroot/modules/contrib/{$name}": ["type:drupal-module"],
          "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
          "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
          "drush/contrib/{$name}": ["type:drupal-drush"]
       }
    },
    "authors": [
        {
            "name": "acouch",
            "email": "acinternets@gmail.com"
        },
        {
          "name": "dafeder",
          "email": "dafeder@gmail.com"
        }
    ]
}
