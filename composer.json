{
    "name": "getdkan/dkan2",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "description": "DKAN Open Data Catalog",
    "require": {
        "cweagans/composer-patches": "^1.5.0",
        "drupal/jsonb": "1.x-dev",
        "drupal/core": "~8.5.2",
        "drupal/entity": "1.0-beta3",
        "drupal/config_update": "1.x-dev",
        "fxp/composer-asset-plugin": "^1.4@dev",
        "bower-asset/json-forms": "dev-master"
    },
    "require-dev": {
      "burdamagazinorg/thunder-dev-tools": "dev-master",
      "drupal/coder": "8.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "enable-asset-packagist": "Social\\Composer\\AssetPackagist::execute"
    },
    "autoload": {
        "psr-4": {
            "Social\\": "src"
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/config_update": {
                "Add Drush 9 support": "https://www.drupal.org/files/issues/2018-06-11/drush9-support-2974637-24.patch"
            }
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "libraries",
                "bower-asset-library": "libraries"
            }
        }
    },
    "authors": [
        {
            "name": "acouch",
            "email": "acinternets@gmail.com"
        },
        {
          "name": "dafeder",
          "email": "dafeder@gmail.com"
        }
    ]
}
