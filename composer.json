{
    "name": "getdkan/dkan2",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "description": "DKAN Open Data Catalog",
    "require": {
        "cweagans/composer-patches": "^1.5.0",
        "drupal/jsonb": "1.x-dev",
        "drupal/core": "~8.5.2",
        "drupal/entity": "1.0-beta3",
        "drupal/config_update": "1.x-dev",
        "bower-asset/json-form": "1.1.11"
    },
    "require-dev": {
      "burdamagazinorg/thunder-dev-tools": "dev-master",
      "drupal/coder": "8.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "enable-asset-packagist": "Social\\Composer\\AssetPackagist::execute"
    },
    "autoload": {
        "psr-4": {
            "Social\\": "src"
        }
    },
    "extra": {
        "enable-patching": true
    },
    "replace": {
      "bower-asset/angular": "*",
      "bower-asset/angular-bootstrap": "*",
      "bower-asset/bootstrap": "*",
      "bower-asset/es5-shim": "*",
      "bower-asset/jquery": "*",
      "bower-asset/json3": "*",
      "bower-asset/ng-file-upload": "*"
    },
    "authors": [
        {
            "name": "acouch",
            "email": "acinternets@gmail.com"
        },
        {
          "name": "dafeder",
          "email": "dafeder@gmail.com"
        }
    ]
}
